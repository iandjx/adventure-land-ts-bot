    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "nodemon": "^2.0.7",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
